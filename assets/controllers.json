{
    "controllers": {
        "@spomky-labs/pwa-bundle": {
            "connection-status": {
                "enabled": true,
                "fetch": "eager"
            },
            "backgroundsync-form": {
                "enabled": true,
                "fetch": "eager"
            },
            "sync-broadcast": {
                "enabled": true,
                "fetch": "eager"
            },
            "prefetch-on-demand": {
                "enabled": true,
                "fetch": "eager"
            }
        },
        "@survos/api-grid-bundle": {
            "js_twig": {
                "enabled": true,
                "fetch": "lazy",
                "autoimport": []
            },
            "api_grid": {
                "enabled": true,
                "fetch": "lazy",
                "autoimport": {
                    "datatables.net-bs5/css/dataTables.bootstrap5.min.css": true,
                    "datatables.net-buttons-bs5/css/buttons.bootstrap5.min.css": true,
                    "datatables.net-responsive-bs5/css/responsive.bootstrap5.min.css": true,
                    "datatables.net-scroller-bs5/css/scroller.bootstrap5.min.css": true,
                    "datatables.net-searchpanes-bs5/css/searchPanes.bootstrap5.min.css": true,
                    "datatables.net-searchbuilder-bs5/css/searchBuilder.bootstrap5.min.css": true,
                    "datatables.net-select-bs5/css/select.bootstrap5.min.css": true
                }
            },
            "grid": {
                "enabled": true,
                "fetch": "lazy",
                "autoimport": {
                    "datatables.net-bs5/css/dataTables.bootstrap5.min.css": true,
                    "datatables.net-buttons-bs5/css/buttons.bootstrap5.min.css": true,
                    "datatables.net-responsive-bs5/css/responsive.bootstrap5.min.css": true,
                    "datatables.net-scroller-bs5/css/scroller.bootstrap5.min.css": true,
                    "datatables.net-searchpanes-bs5/css/searchPanes.bootstrap5.min.css": true,
                    "datatables.net-searchbuilder-bs5/css/searchBuilder.bootstrap5.min.css": true,
                    "datatables.net-select-bs5/css/select.bootstrap5.min.css": true
                }
            }
        },
        "@survos/pwa-extra-bundle": {
            "detector": {
                "enabled": true,
                "fetch": "lazy",
                "autoimport": []
            },
            "toggle_online": {
                "enabled": true,
                "fetch": "lazy",
                "autoimport": []
            },
            "install": {
                "enabled": true,
                "fetch": "lazy",
                "autoimport": []
            }
        },
        "@survos/workflow-helper-bundle": {
            "workflow": {
                "enabled": true,
                "fetch": "eager"
            }
        },
        "@symfony/ux-turbo": {
            "turbo-core": {
                "enabled": true,
                "fetch": "eager"
            },
            "mercure-turbo-stream": {
                "enabled": false,
                "fetch": "eager"
            }
        }
    },
    "entrypoints": []
}
